<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ModelUnit PUBLIC " "
        "https://raw.githubusercontent.com/AgriculturalModelExchangeInitiative/crop2ml/master/ModelUnit.dtd">
<ModelUnit modelid="Monica_Evapotranspiration.PotentialEvapotranspiration"
           name="PotentialEvapotranspiration"
           timestep="1" version="1">
    <Description>
        <Title>MONICA potential evapotranspiration calculation</Title>
        <Authors>Claas Nendel (transcription into Crop2ML by Michael Berg-Mohnicke)</Authors>
        <Institution>ZALF e.V.</Institution>
        <URI></URI>
        <Reference></Reference>
        <ExtendedDescription></ExtendedDescription>
        <ShortDescription>Calculates the MONICA potential evapotranspiration</ShortDescription>
    </Description>
    <Inputs>
        <!-- exogenous -->
        <Input name="external_reference_evapotranspiration" description="externally supplied ET0"
               inputtype="variable" variablecategory="exogenous"
               datatype="DOUBLE" max="" min="0" default="-1" unit="mm"/>
        <Input name="kc_factor" description="crop coefficient ETc/ET0"
               inputtype="variable" variablecategory="exogenous"
               datatype="DOUBLE" max="" min="0" default="0.75" unit="dimensionless"/>
        <Input name="developmental_stage" description="MONICA crop developmental stage"
               inputtype="variable" variablecategory="exogenous"
               datatype="INT" max="6" min="0" default="0" unit="dimensionless"/>
        <Input name="crop_reference_evapotranspiration"
               description="the crop specific ET0, if no external ET0 and crop is planted"
               inputtype="variable" variablecategory="exogenous"
               datatype="DOUBLE" max="" min="0" default="-1" unit="mm"/>
        <Input name="crop_remaining_evapotranspiration" description="crop remaining evapotranspiration"
               inputtype="variable" variablecategory="exogenous"
               datatype="DOUBLE" max="" min="" default="" unit="mm"/>

        <!-- auxiliary -->
        <Input name="reference_evapotranspiration" description="ET0"
               inputtype="variable" variablecategory="auxiliary"
               datatype="DOUBLE" max="" min="0" default="0" unit="mm"/>

        <!-- states -->
    </Inputs>
    <Outputs>
        <Output name="potential_evapotranspiration"
                description="the potential evapotranspiration"
                variablecategory="auxiliary"
                datatype="DOUBLE" max="" min="0" unit="mm"/>
        <Output name="reference_evapotranspiration" description="ET0"
                variablecategory="auxiliary"
                datatype="DOUBLE" max="" min="0" unit="mm"/>
    </Outputs>
    <!-- if there is no state, there is no need for an initialization function -->
<!--    <Initialization name="init_potential_evapotranspiration" language="cyml"-->
<!--                    filename="algo/pyx/init_potential_evapotranspiration.pyx"/>-->
    <Algorithm language="cyml" platform=""
               filename="algo/pyx/potential_evapotranspiration.pyx"/>
    <Parametersets/>
    <Testsets/>
</ModelUnit>